<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="611.0" prefWidth="796.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mikhail.pravilov.mit.ftpClient.controller.FileManagerSceneController">
   <children>
      <VBox layoutY="-1.0" prefHeight="611.0" prefWidth="796.0">
         <children>
            <HBox prefHeight="46.0" prefWidth="601.0">
               <children>
                  <Button mnemonicParsing="false" onMouseClicked="#backButtonMouseClicked" text="Back">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <TextField fx:id="pathTextField" prefHeight="26.0" prefWidth="693.0">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <Label prefHeight="32.0" prefWidth="98.0" text="Directories" />
            <ListView fx:id="directoriesListView" onMouseClicked="#directoriesListViewMouseClicked" prefHeight="252.0" prefWidth="796.0" />
            <Label prefHeight="49.0" prefWidth="94.0" text="Files" />
            <ListView fx:id="filesListView" onMouseClicked="#filesListViewMouseClicked" prefHeight="238.0" prefWidth="796.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
