<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="150.0" prefWidth="224.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mikhail.pravilov.mit.ftpClient.controller.SettingUpConnectionSceneController">
   <children>
      <VBox alignment="CENTER" layoutX="3.0" prefHeight="150.0" prefWidth="250.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <children>
                  <Label text="Host">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin></Label>
                  <TextField fx:id="hostTextField">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin></TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label text="Port">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="portTextField" />
               </children></HBox>
            <Button mnemonicParsing="false" onMouseClicked="#ConnectButtonClicked" text="Connect" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
